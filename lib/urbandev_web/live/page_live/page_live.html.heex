            <div class=" mt-60 wow fadeInUp" data-wow-duration="1s" data-wow-delay="0.6s">
               <.form
                    let={f}
                    for={@changeset}
                    id= "access-form"
                    phx-change="validate"
                    phx-submit="save">
                            <div class="row">
                                <div class="col-md-4">
                                    <div class="contact-form mt-45">
                                        <label>Enter Your Name</label>
                                        <%= text_input f, :names,class: "form-control" , style: "background:#09ce03",placeholder: "enter your full names",required: "required"%>
                                          <span class="btn-danger"><%= error_tag f, :names %></span>
                                    </div> <!-- contact-form -->
                                </div>
                                <div class="col-md-4">
                                    <div class="contact-form mt-45">
                                        <label>Enter Your Phone number</label>
                                         <%= number_input f, :phone,class: "form-control" , style: "background:#09ce03",placeholder: "enter your phone #",required: "required"%>
                                          <span class="btn-danger"><%= error_tag f, :phone %></span>
                                    </div> <!-- contact-form -->
                                </div>
                                <div class="col-md-4">
                                    <div class="contact-form mt-45">
                                        <label>Enter Your ID number</label>
                                         <%= text_input f, :nationalid ,class: "form-control" , style: "background:#09ce03",placeholder: "enter your id number",required: "required"%>
                                        <span class="btn-danger"> <%= error_tag f, :nationalid  %></span>
                                    </div> <!-- contact-form -->
                                </div>
                                <div class="col-md-6">
                                    <div class="contact-form mt-45">
                                        <label>Enter Location</label>
                                        <%= select f, :visiting, @estate, class: "form-control", style: "background:#09ce03", title: "Choose item" , required: "required"%>

                                        <span class="btn-danger"> <%= error_tag f, :visiting %></span>
                                    </div> <!-- contact-form -->
                                </div>


                                <div class="col-md-6">
                                  <div class="contact-form mt-45">
                                    <label for="res" class="form">type</label>
                                    <%= select f, :type,["Walk-ins": "Walk-ins", Driving: "Driving"], class: "form-control", style: "background:#09ce03",style: "width:100%;", title: "Choose item" , required: "required"%>

                                  <%= error_tag f, :type %>
                                  </div>
                                </div>
                                <div class="col-md-12">
                                    <div class="contact-form mt-45">
                                        <label>Purpose of visit</label>
                                         <%= textarea f, :description,class: "form-control" , style: "background:#09ce03",placeholder: "Enter your purpose",required: "required"%>
                                          <span class="btn-danger"><%= error_tag f, :description %></span>
                                    </div> <!-- contact-form -->
                                </div>
                                <p class="form-message"></p>
                                <div class="col-md-12">
                                    <div class="contact-form mt-45">
                                        <%= submit "Save", phx_disable_with: "Saving..." ,class: "main-btn"%>
                                    </div> <!-- contact-form -->
                                </div>
                            </div> <!-- row -->
                        </.form>
                         <div id ="hideMsg">
                        <p class="alert alert-info" role="alert"
                          phx-click="lv:clear-flash"
                          phx-value-key="info"><%= live_flash(@flash, :info) %></p>

                        <p class="alert alert-danger" role="alert"
                          phx-click="lv:clear-flash"
                          phx-value-key="error"><%= live_flash(@flash, :error) %></p>
                        </div>
            </div>
        </div>
     </section>
