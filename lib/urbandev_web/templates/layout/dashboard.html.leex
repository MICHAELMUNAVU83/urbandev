            <div class="navbar-header">
                <a class="navbar-brand" href="/dashboard">
                    <!-- Logo icon --><b>
                        <!--You can put here icon as well // <i class="wi wi-sunset"></i> //-->

                        <!-- Light Logo icon -->
                        <img src="<%= Routes.static_path(@socket, "/images/jasusi.png") %>" width= "75px"  alt="homepage" class="light-logo" />
                    </b>
                    <!--End Logo icon -->
                    <!-- Logo text -->
                  </a>
                     <!-- Light Logo text -->

            </div>
