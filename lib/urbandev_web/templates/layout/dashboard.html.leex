      <script language="javascript" type="text/javascript">
  function timeMsg(){

    window.setTimeout("clearMsg()",5000);//10secs

  }

  //

  function clearMsg(){

    document.getElementById("hideMsg").innerHTML = "";

  }
</script>

</head>

<body onLoad = "timeMsg()"  class="fix-header fix-sidebar card-no-border">

  <head>
